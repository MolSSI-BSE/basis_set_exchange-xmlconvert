<?xml version="1.0" encoding="UTF-8"?>

<basisSet 
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:cml="http://www.xml-cml.org/schema/cml2/core"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns="http://purl.oclc.org/NET/EMSL/BSE">

  <dc:format>chemical/x-emsl-gaussian-basisset</dc:format>
  <dc:title>Glendening Polarization</dc:title>
  <dct:abstract>1P      Polarization functions</dct:abstract>
  <conversion>true</conversion>
  <basisSetType>polarization</basisSetType>
  <harmonicType>spherical</harmonicType>
  <contractionType>segmented</contractionType>

  <dc:description><![CDATA[              Glendening/Feller/Thompson Polarization Sets
              --------------------------------------------
 Elements                            Reference
K, Rb, Cs(1d)  E. Glendening, D. Feller and M. Thompson, J. Am. Chem. Soc.
               116, 10657 (1994).

]]></dc:description>

  <referencesLink xlink:type="simple" xlink:href="GLENDENING_POL-BS-REF.xml"/>

  <contractions elementType="K">
    <dc:description>(1d)</dc:description>
    <contraction shell="D">
      <cml:matrix rows="1" columns="2" dataType="xs:double">
0.48 1.000000 
       </cml:matrix>
    </contraction>
  </contractions>

  <contractions elementType="Rb">
    <dc:description>(1d)</dc:description>
    <contraction shell="D">
      <cml:matrix rows="1" columns="2" dataType="xs:double">
0.24 1.000000 
       </cml:matrix>
    </contraction>
  </contractions>

  <contractions elementType="Cs">
    <dc:description>(1d)</dc:description>
    <contraction shell="D">
      <cml:matrix rows="1" columns="2" dataType="xs:double">
0.19 1.000000 
       </cml:matrix>
    </contraction>
  </contractions>


</basisSet>

