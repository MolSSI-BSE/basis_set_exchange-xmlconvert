    <contraction shell="D">
      <cml:matrix rows="1" columns="2" dataType="xs:double">
0.235000 1.000000 
       </cml:matrix>
    </contraction>
  </contractions>

  <contractions elementType="Ca">
    <dc:description>(1d)</dc:description>
    <contraction shell="D">
      <cml:matrix rows="1" columns="2" dataType="xs:double">
0.216000 1.000000 
       </cml:matrix>
    </contraction>
